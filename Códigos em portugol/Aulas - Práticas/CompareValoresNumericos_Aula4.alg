algoritmo "CompareValoresNumericos_Aula4"
var
   A, B, C: Inteiro
inicio
      A <- 2
      B <- 3
      C <- 5
      EscrevaL (A > B)
      EscrevaL (A = B)
      EscrevaL (A <> B)
      EscrevaL (A > 2)
      EscrevaL (A >= 2)
      EscrevaL (C=A+B)
      EscrevaL (C >= B^A)
      EscreVaL (C <= B^A)
      EscrevaL (B <= C%2)
      EscrevaL (1 = C%2)
fimalgoritmo
