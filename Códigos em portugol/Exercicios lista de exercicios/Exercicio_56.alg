algoritmo "Exercicio_56"
var
   numInterrupcao, N, Soma: Inteiro
inicio
numInterrupcao <- 0
      Enquanto (numInterrupcao <> 1111) faca
               Escreva("Digite um número: ")
               Leia(N)
               numInterrupcao <- N
               Soma <- Soma + N
      FimEnquanto
      EscrevaL("A soma dos números digitados antes da interrupção foi: ", Soma)
fimalgoritmo
