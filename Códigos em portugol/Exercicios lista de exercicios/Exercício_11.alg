algoritmo "Exercício_11"
var
   A, B, C: Inteiro
   Delta: Real
inicio
      Escreva("Digite o valor A:")
      Leia(A)
      Escreva("Digite o valor B:")
      Leia(B)
      Escreva("Digite o valor C:")
      Leia(C)
      Delta <- (b^2) - (4 * (A * C))/ 2 * A
      Escreva("O delta é: ", Delta)
fimalgoritmo
